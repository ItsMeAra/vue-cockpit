{
  "name": "vue-cockpit",
  "version": "0.5.1",
  "description": "Makes it easy to build a frontend displaying data from Cockpit CMS by automatically mapping all collections and regions into variables in the vue data scope.",
  "main": "src/index.js",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "build:umd": "webpack --output-filename vue-cockpit.js",
    "build:umd.min": "webpack --output-filename vue-cockpit.min.js -p",
    "prebuild": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dropclick/vue-cockpit.git"
  },
  "keywords": [
    "vue",
    "cockpit",
    "cms",
    "frontend"
  ],
  "author": "Sven Petersen <mail@svenpetersen.net> (http://svenpetersen.net/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dropclick/vue-cockpit/issues"
  },
  "homepage": "https://github.com/dropclick/vue-cockpit#readme",
  "dependencies": {
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "webpack": "1.13.1"
  }
}
